{
  "semi": true,
  "trailingComma": "all",
  "jsxSingleQuote": true,
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": true,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "insertPragma": false,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "endOfLine": "lf"
}